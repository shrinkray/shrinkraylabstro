---
import { GoogleAnalytics } from '@astrolib/analytics';
import ANALYTICS from '~/config/analytics';
import SplitbeeAnalytics from './SplitbeeAnalytics.astro';
---

<!-- Google Analytics -->{
  ANALYTICS.googleAnalyticsId && <GoogleAnalytics id={String(ANALYTICS.googleAnalyticsId)} partytown={true} />
}

<!-- Splitbee Analytics -->
{ANALYTICS.splitbeeAnalytics?.enabled && <SplitbeeAnalytics {...ANALYTICS.splitbeeAnalytics} />}
